package com.simplecalc;

//import java.util.Scanner;

public class SimpleCalc {

	public static void main(String[] args) {
	
		for(int i = 1; i <= 4; i++) {
			for(int j = 1; j <= i; j ++) {
				System.out.print("*");
			}
		System.out.println();
		}
		for(int x = 3; x <= 1; x --) {
			for(int y = 3; y <= x; y++)
				System.out.println("*");
		}
		System.out.println();
	}
}